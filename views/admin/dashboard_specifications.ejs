    <link rel="stylesheet" href="partials/css.css">
</head>

<body>
    <!-- Start: Navigation with Search -->
    <nav class="navbar navbar-light navbar-expand-md navigation-clean-search">
        <div class="container">
            <img style="width: 83px;" src="img/up-logo.webp?h=8dfc11d7a064a0135a4ede4c92194ea1">
            <button data-bs-toggle="collapse" class="navbar-toggler" data-bs-target="#navcol-1">
                <span class="visually-hidden">Toggle navigation</span>
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navcol-1">
                <ul class="navbar-nav">
                    <li class="nav-item"><a class="nav-link active" href="#">תמיכה טכנית</a></li>
                    <li class="nav-item"><a class="nav-link" href="#">עלינו</a></li>
                    <li class="nav-item"><a class="nav-link" onclick="window.location.replace(window.location.protocol + '//' + window.location.host + '/admin/confirmPayments')">אישור עסקאות</a></li>
                    <li class="nav-item"><a class="nav-link" href="#">עמוד הבית</a></li>
                </ul>
                <form class="me-auto search-form" target="_self">
                    <div class="d-flex align-items-center">
                        <label class="form-label d-flex mb-0" for="search-field">
                            <i class="fa fa-search"></i>
                        </label>
                        <input class="form-control search-field" type="search" id="search-field" name="search">
                    </div>
                </form>
                <!-- <a class="btn btn-light action-button" role="button" href="#">פנאל ניהול</a> -->
            </div>
        </div>
    </nav><!-- End: Navigation with Search -->

    <%- include ../partials/messages.ejs %>
    <div class="alerts"></div>

    <div class="table-responsive">
        <table class="table">
            <thead style="text-align: center;">
                <tr>
                    <th>'עסקה מס</th>
                    <th>'קורס מס</th>
                    <th>טלפון מדריך</th>
                    <th>שם מדריך</th>
                    <th>טלפון מזמין</th>
                    <th>ארגון המזמין</th>
                    <th>שם מזמין</th>
                    <th>תאריך הקורס</th>
                    <th>תאריך עדכון איפיון</th>
                    <th>שם הקורס</th>
                </tr>
            </thead>
            <tbody style="text-align: center;">
                <% Specifications.forEach(Specification => { %>
                <tr style="text-align: center;" id="SpecificationRow<%= Specification['id'] %>">
                    <td><%= Specification["TransactionID"] %></td>
                    <td><%= Specification["Number"] %></td>
                    <td><%= Specification["CourseInstructorPhone"] %></td>
                    <td><%= Specification["CourseInstructorName"] %></td>
                    <td><%= Specification["UserPhoneNumber"] %></td>
                    <td><%= Specification["UserOrganization"] %></td>
                    <td><%= Specification["UserName"] %></td>
                    <td><%= Specification["ExpiredCourseTimeDate"] %></td>
                    <td><%= Specification["Date"] %></td>
                    <td><%= Specification["CourseName"] %></td>
                </tr>
                <% }) %>
            </tbody>
        </table>
    </div>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
    <script src="bootstrap/js/bootstrap.min.js?h=06ed58a0080308e1635633c2fd9a56a3"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"></script>
    <script src="js/moment.js"></script>
    <script src="js/utils.js"></script>
    <script src="js/admin_dashboard_payments.js"></script>
</body>

</html>