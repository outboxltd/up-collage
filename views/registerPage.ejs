<body>
    <!-- Start: Registration Form with Photo -->
    <section class="register-photo">
        <!-- Start: Form Container -->
        <%- include partials/messages.ejs %>
        <div class="form-container">
            <!-- Start: Image -->
            <div class="image-holder"
                style="background: url(&quot;img/rsz_1working-as-team-peeugkk%20(1).jpeg?h=977a47552929e9f7573bad8c84e20f4b&quot;) center;">
            </div><!-- End: Image -->
            <form id="registerform" action="/" method="post">
                <img src="img/up-logo.webp?h=8dfc11d7a064a0135a4ede4c92194ea1"
                    style="text-align: right;margin-right: 0px;">
                <h2 class="text-center"><strong>קבלת פרטים על המערכת</strong></h2>
                <div class="mb-3"></div>
                <div class="mb-3"></div>
                <div class="mb-3" style="text-align: center;">
                    <input class="form-control" type="text" id="fullname"
                        style="margin: 0px;margin-bottom: 10px;text-align: right;" name="FullName" placeholder="שם מלא"
                        value="<%= typeof FullName != 'undefined' ? FullName : '' %>">

                    <input class="form-control" type="text" id="org"
                        style="margin: 0px;margin-bottom: 10px;text-align: right;" name="OrganizationName"
                        placeholder="שם הארגון שלך"
                        value="<%= typeof OrganizationName != 'undefined' ? OrganizationName : '' %>">

                    <input class="form-control" type="text" id="phone"
                        style="margin: 0px;margin-bottom: 10px;text-align: right;" name="PhoneNumber"
                        placeholder="מספר הטלפון שלך"
                        value="<%= typeof PhoneNumber != 'undefined' ? PhoneNumber : '' %>">

                    <input class="form-control" type="email" placeholder="כתובת המייל שלך"
                        style="text-align: right;margin-bottom: 10px;" name="EmailAddress"
                        value="<%= typeof EmailAddress != 'undefined' ? EmailAddress : '' %>">

                    <input class="form-control" type="password" placeholder="סיסמה"
                        style="text-align: right;margin-bottom: 10px;" name="Password"
                        value="<%= typeof Password != 'undefined' ? Password : '' %>">

                    <input class="form-control" type="password" placeholder="אישור סיסמה"
                        style="text-align: right;margin-bottom: 10px;" name="Password2"
                        value="<%= typeof Password2 != 'undefined' ? Password2 : '' %>">

                    <input class="form-control" type="number" placeholder="מספר צוותים בארגון"
                        style="text-align: right;margin-bottom: 10px;" name="TeamsAtOrganization"
                        value="<%= typeof TeamsAtOrganization != 'undefined' ? TeamsAtOrganization : '' %>">

                    <textarea class="form-control" placeholder="הערות כלליות" style="text-align: right;height: 56px;"
                        name="GeneralNotes"
                        value="<%= typeof GeneralNotes != 'undefined' ? GeneralNotes : '' %>"></textarea>

                    <label class="form-label" style="text-align: right;margin-top: 10px;">
                        במידה ונדרשת סדנה עבור יותר מצוות אחד אנא רשמו זאת בהערות
                    </label>

                </div>

                <div class="mb-3"><button class="btn btn-primary d-block w-100" type="submit">חזרו אליי עם
                        פרטים</button></div>
                <a class="already" href="/login">?רשומים כבר למערכת</a>
            </form>
        </div><!-- End: Form Container -->
    </section><!-- End: Registration Form with Photo -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="bootstrap/js/bootstrap.min.js?h=06ed58a0080308e1635633c2fd9a56a3"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"></script>
    <!-- <script src="js/register.js"></script> -->
</body>

</html>