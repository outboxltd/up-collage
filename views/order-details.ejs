        href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/css/bootstrap-datepicker.min.css"
        integrity="sha512-mSYUmp1HYZDFaVKK//63EcZq4iFWFjxSL+Z3T/aCt4IO9Cejm03q3NKKYN6pFQzY0SBOr8h+eCIAZHPXcpZaNw=="
        crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="stylesheet" href="partials/css.css">
</head>

<body>
    <!-- Start: Registration Form with Photo -->
    <section class="register-photo">
        <!-- Start: Form Container -->

        <%- include partials/messages.ejs %>
        <div class="alerts"></div>

        <div class="form-container">

            <!-- Start: Image -->
            <div class="image-holder"
                style="background: url(&quot;img/rsz_gifts-n6s4qcg%20(1).jpeg?h=4ee32e391d0b335f3aa87b09ec7697fd&quot;) center;width: 50%;height: 56%;">
            </div><!-- End: Image -->
            <form method="post" style="padding: 39px 60px;">
                <!-- <h2 class="text-center"><strong>שם הארגון : עירייה נהריה</strong></h2> -->
                <h2 class="text-center" style="margin-bottom: 0px;"><strong>מספר רכישה : <%= Transaction.id %></strong>
                </h2>
                <br>
                
                <div class="mb-3" style="text-align: center;">

                    <input class="form-control" type="text"
                        style="margin: 0px;margin-bottom: 10px;text-align: right;" name="CourseInstructorName"
                        placeholder="שם מוביל הסדנה">

                    <input class="form-control" type="text"
                        style="margin: 0px;margin-bottom: 10px;text-align: right;" name="CourseInstructorPhone"
                        placeholder="טלפון מוביל סדנה">

                    <input class="form-control" type="email"
                        style="margin: 0px;margin-bottom: 10px;text-align: right;" name="CourseInstructorEmail"
                        placeholder="מייל מוביל סדנה">

                    <input class="form-control" placeholder="תאריך - יום ושעה לביצוע הסדנה"
                        style="text-align: right;margin-bottom: 10px;" name="ExpiredCourseTimeDate" readonly>

                    <input class="form-control" placeholder="כתובת מדוייקת"
                        style="text-align: right;margin-bottom: 10px;" name="Address">
                </div>

                <a class="already" href="#" style="text-align: center;">לכתובת זו תשלח הערכה עם כל האבזרים הנדרשים עד
                    יומיים לפני הסדנה</a>

                <input class="form-control" type="number" placeholder="מספר משתתפים בסדנה"
                    style="text-align: right;margin-bottom: 10px;" name="NumberOfCourseParticipants">

                <div class="mb-3"></div>

                <textarea class="form-control" placeholder="הערות כלליות"
                    style="text-align: right;height: 56px;" name="GeneralNotes"></textarea>

                <input type="hidden" name="ProductID" value="">
                <input type="hidden" name="TransactionID" value="<%= Transaction.id %>">

                <div class="mb-3">
                    <button class="btn btn-primary d-block w-100" onclick="checks()">עדכון פרטים</button>
                </div>
                <a class="already" href="#">שימו לב לאחר עדכון הפרטים לא ניתן לשנות את פרטי ההזמנה לסדנה זאת אנא בדקו
                    לפני שליחה</a>
            </form>
        </div><!-- End: Form Container -->
    </section><!-- End: Registration Form with Photo -->

    <script src="https://code.jquery.com/jquery-3.6.0.js"
        integrity="sha256-H+K7U5CnXl1h5ywQfKtSj8PCmoN9aaq30gDh27Xc0jk=" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"></script>
    <script src="bootstrap/js/bootstrap-datetimepicker.js"></script>
    <script src="bootstrap/js/bootstrap-datetimepicker.he.js"></script>
    <script src="js/utils.js"></script>
    <script src="js/orderDetails.js"></script>
</body>

</html>